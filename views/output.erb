<span class="label label-info">Stats:</span>
 <%= @total %> courses

<table class="courses table table-striped table-bordered">
    
    <% @courses.each do |program| %>
        <% program.each do |course| %>
            <tr>
                <% course.each do |data| %>
                    <td><%= data %></td>
                <% end %>
            </tr>
        <% end %>
    <% end %>
</table>


<form action="download" method="post">
    <input type="hidden" name="courses" value="<%= @courses.to_s.tr('"',"'") %>">
    <input type="submit" class="btn">
</form>
