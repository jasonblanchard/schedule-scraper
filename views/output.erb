
<div class="top-bar">
    <div id="stats">
        <span class="label label-info">Stats:</span>
        <%= @total %> courses
    </div>
    <a class="btn btn-success" id="download-button" href="files/<%= "#{@timestamp}.xlsx" %>">Download</a>
</div>


<table class="courses table table-striped table-bordered">
    
    <% @courses.each do |program| %>
        <% program.each do |course| %>
            <tr>
                <% course.each do |data| %>
                    <td><%= data %></td>
                <% end %>
            </tr>
        <% end %>
    <% end %>
</table>
